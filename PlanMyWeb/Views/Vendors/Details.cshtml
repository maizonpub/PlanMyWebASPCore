<div class="spacer">
    <div class="container">
        <div class="row">
            
            @if (Model.RelatedItems != null)
            {
            <div class="col-md-12 tp-title">
                <h1 style="color:black;">Recommended for&nbsp;@Model.VendorItem.Title </h1>
            </div>
                foreach (var row in Model.RelatedItems.ToList())
                {
                    


                        <div class="col-md-3 vendor-box">
                            <!-- venue box start-->
                            <div class="vendor-image">
                                <!-- venue pic -->
                                <a href="/Vendors/Details/@row.Id">
                                    <img width="440" height="290" src="~/Media/@row.Thumb" class="img-responsive wp-post-image" alt="" />
                                </a>
                            </div>
                            <!-- /.venue pic -->
                            <div class="vendor-detail">
                                <!-- venue details -->
                                <div class="caption">
                                    <!-- caption -->
                                    <h4><a href="/Vendors/Details/@row.Id" class="title">@row.Title</a></h4>
                                    <p class="location"><i class="fa fa-map-marker"></i> @row.Address</p>
                                </div>
                                <!-- /.caption -->
                            </div>
                            <!-- venue details -->
                        </div>
                    
                }
            }
        </div>
    </div>
</div>




