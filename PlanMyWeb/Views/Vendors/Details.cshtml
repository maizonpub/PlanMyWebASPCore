@model SingleVendorViewModel

@{
    ViewData["Title"] = "Details";
    string facebook = "", instagram = "", website = "";
    var reviews = Model.VendorItem.VendorItemReviews.Where(x => x.Status == DAL.ReviewStatus.Approved);
    if (!string.IsNullOrEmpty(Model.VendorItem.Facebook))
    {
        facebook = Model.VendorItem.Facebook.Contains("http") ? Model.VendorItem.Facebook : "https://" + Model.VendorItem.Facebook;
    }
    if (!string.IsNullOrEmpty(Model.VendorItem.Instagram))
    {
        instagram = Model.VendorItem.Instagram.Contains("http") ? Model.VendorItem.Instagram : "https://" + Model.VendorItem.Instagram;
    }
    if (!string.IsNullOrEmpty(Model.VendorItem.Website))
    {
        website = Model.VendorItem.Website.Contains("http") ? Model.VendorItem.Website : "https://" + Model.VendorItem.Website;
    }
}
