                    <ol class="carousel-indicators">
                        @for (int i = 0; i < Model.VendorItem.Gallery.Count(); i++)
                        {
                            string isactive = i == 0 ? "active" : "";

                            <li data-target="#myCarousel" data-slide-to="@i" class="@isactive"></li>

