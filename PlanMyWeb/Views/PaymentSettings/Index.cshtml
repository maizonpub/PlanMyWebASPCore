@model IEnumerable<DAL.PaymentSetting>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}



<p>
    <a asp-action="Create" class="btn btn-primary">Create New</a>
</p>
<div class="col-xs-12">
    <div class="box">
        <div class="box-body">
            <table id="example2" class="table table-bordered table-hover">
                <thead>
                    <tr>
                        <th>
                            @Html.DisplayNameFor(model => model.ProfileId)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.AccessKey)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.SecuritySign)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.PaymentType)
                        </th>
                        <th>
                            @Html.DisplayNameFor(model => model.RecurringFrequency)
                        </th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => item.ProfileId)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.AccessKey)
                            </td>
                            <td style="word-wrap: break-word; word-break: break-all">
                                @Html.DisplayFor(modelItem => item.SecuritySign)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.PaymentType)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => item.RecurringFrequency)
                            </td>
                            <td>
                                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
</div></div></div>