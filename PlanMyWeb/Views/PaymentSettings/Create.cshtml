@model DAL.PaymentSetting

@{
    ViewData["Title"] = "Create";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>Create</h2>

<h4>PaymentSetting</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ProfileId" class="control-label"></label>
                <input asp-for="ProfileId" class="form-control" />
                <span asp-validation-for="ProfileId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AccessKey" class="control-label"></label>
                <input asp-for="AccessKey" class="form-control" />
                <span asp-validation-for="AccessKey" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SecuritySign" class="control-label"></label>
                <input asp-for="SecuritySign" class="form-control" />
                <span asp-validation-for="SecuritySign" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PaymentType" class="control-label"></label>
                <select asp-for="PaymentType" asp-items="Html.GetEnumSelectList<DAL.PaymentType>()"  class="form-control"></select>
                <span asp-validation-for="PaymentType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="RecurringFrequency" class="control-label"></label>
                <input asp-for="RecurringFrequency" class="form-control" />
                <span asp-validation-for="RecurringFrequency" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

